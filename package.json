    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-svelte-csf": "^2.0.3",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/svelte": "^6.4.22",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
